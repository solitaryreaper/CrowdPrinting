# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.LandingController.index()
GET		/login						controllers.LoginController.login()
GET		/join						controllers.JoinController.createID()
GET     /upload1                     controllers.UploadController.uploadStep1()
GET     /upload2                     controllers.UploadController.uploadStep2()
GET     /upload3                     controllers.UploadController.uploadStep3()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
